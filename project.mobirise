            },
            ".card__text": {
              "letter-spacing": "0",
              "margin": "10px 0"
            },
            ".btn": {
              "margin-top": "20px"
            },
            "@media (max-width: 600px)": {
              ".title__block": {
                "margin-bottom": "40px"
              },
              ".card .image": {
                "height": "(@imgHeight * 1px)"
              },
              ".card": {
                "padding-bottom": "0"
              }
            }
