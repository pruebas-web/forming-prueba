        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 10px)",
            "padding-bottom": "(@paddingBottom * 10px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              "& when (@overlay)": {
                ".mbr-overlay": {
                  "background": "@overlayColor",
                  "opacity": "@overlayOpacity"
                }
              }
            },
            ".mbr-section-title": {
              "margin-top": "10px",
              "line-height": "1.2em"
            },
            ".mbr-section-subtitle": {
              "margin-top": "10px",
              "color": "#9b9b9b"
            },
            ".contacts__text": {
              "width": "100%",
              "padding-right": "30px",
              ".contacts-text": {
                "padding": "20px 0"
              }
            },
            ".contacts-box": {
              "width": "100%",
              "margin-top": "20px",
              "padding-left": "40px"
            },
            ".adress__box": {
              "margin-bottom": "20px"
            },
            ".contacts__callback": {
              "width": "100%"
            },
            "& when (@showEmail) and (@showPhone)": {
              ".contacts__callback-left": {
                "margin-right": "20px",
                "width": "50%"
              },
              ".contacts__callback-right": {
                "width": "50%"
              }
            },
            "& when not (@showEmail), not (@showPhone)": {
              ".contacts__callback-left, .contacts__callback-right": {
                "width": "100%"
              }
            },
            ".contacts-span": {
              "letter-spacing": "2px",
              "color": "#9b9b9b"
            },
            ".btn": {
              "transform": "scale(1) !important"
            },
            ".mbr-section-btn": {
              "margin-top": "20px"
            },
            ".contacts-block": {
              "width": "100%",
              "@media (max-width: 768px)": {
                "flex-direction": "column",
                "align-items": "center",
                ".contacts__text": {
                  "padding-right": "0px"
                },
                ".contacts-box": {
                  "margin-top": "40px",
                  "padding-left": "0px"
                },
                ".contacts__callback": {
                  "flex-direction": "column"
                },
                ".contacts__callback-left, .contacts__callback-right": {
                  "width": "100%",
                  "margin-bottom": "20px"
                }
              }
            }
