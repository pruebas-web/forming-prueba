              "border": "10px solid @BorderColor",
              "transition": "all 0.35s",
              "& when not (@BorderItems)": {
                "border": "none"
              },
              "&:hover": {
                "transform": "scale(1.1)",
                "&:after": {
                  "opacity": "@imgOverlayOpacity"
                }
              },
              "&:after": {
                "background": "@imgOverlayColor"
