                ".inner-item": {
                  "background-color": "@cardBG",
                  "position": "relative",
                  "height": "100%"
                }
              },
              ".image": {
                "height": "(@imgHeight * 1px)",
                "min-height": "200px",
                "width": "100%",
                "& when (@showEffect)": {
                  "position": "relative",
                  "&:before": {
                    "content": "\"\"",
                    "position": "absolute",
                    "top": "0",
                    "right": "0",
                    "left": "0",
                    "bottom": "0",
                    "z-index": "1",
                    "pointer-events": "none",
                    "transition": "all .4s ease",
                    "border-top-left-radius": "5px",
                    "border-top-right-radius": "5px"
                  },
                  "amp-img": {
                    "border-top-left-radius": "5px",
                    "border-top-right-radius": "5px",
                    "overflow": "hidden",
                    "object-fit": "cover",
                    "&:before, &:after": {
                      "content": "''",
                      "background-color": "contrast(@imgOverlayColor)",
                      "position": "absolute",
                      "z-index": "1",
                      "top": "50%",
                      "left": "50%",
                      "opacity": "0",
                      "-webkit-transform": "translate(-50%, -50%)",
                      "transform": "translate(-50%, -50%)",
                      "-webkit-transition": "all .4s ease",
                      "transition": "all .4s ease"
                    },
                    "&:before": {
                      "left": "100%",
                      "width": "60px",
                      "height": "1px"
                    },
                    "&:after": {
                      "top": "0",
                      "width": "1px",
                      "height": "60px"
                    }
                  },
                  "&:hover:before": {
                    "background-color": "@imgOverlayColor",
                    "opacity": "@imgOverlayOpacity"
                  },
                  "&:hover amp-img:before, &:hover amp-img:after": {
                    "opacity": "1"
                  },
                  "&:hover amp-img:before": {
                    "left": "50%"
                  },
                  "&:hover amp-img:after": {
                    "top": "50%"
                  }
                }
